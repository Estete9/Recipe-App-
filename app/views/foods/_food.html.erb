<div id="<%= dom_id food %>">
<table class="table table-border table-striped" id="<%= dom_id food %>">
<tbody>
  <tr>
    <th>Name</th>
    <td><%= food.name %></td>
  </tr>
  <tr>
    <th>Measurement unit</th>
    <td><%= food.measurement_unit %></td>
  </tr>
  <tr>
    <th>Price</th>
    <td><%= food.price %></td>
  </tr>
  <tr>
    <th>User</th>
    <td><%= food.user.name %></td>
  </tr>
</tbody>
</table>
</div>
